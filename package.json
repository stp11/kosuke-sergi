{
  "name": "kosuke-template",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:email": "concurrently \"bun run dev\" \"bun run email:dev\"",
    "dev:all": "concurrently \"bun run dev\" \"bun run workers:dev\"",
    "workers:dev": "bun tsx --watch -r dotenv/config worker.ts",
    "workers:start": "bun tsx -r dotenv/config worker.ts",
    "prebuild": "bun run db:migrate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:all": "bun run lint && bun run lint:engine",
    "lint:engine": "cd engine && uv run ruff check .",
    "lint:engine:fix": "cd engine && uv run ruff check . --fix",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "test:all": "bun run test && bun run test:engine",
    "test:engine": "cd engine && uv run pytest -v",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:all": "bun run format && bun run format:engine",
    "format:engine": "cd engine && uv run ruff format .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "typecheck:all": "bun run typecheck && bun run typecheck:engine",
    "typecheck:engine": "cd engine && uv run mypy .",
    "check:all": "bun run lint:all && bun run typecheck:all && bun run test:all && bun run knip",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run lib/db/scripts/db-seed.ts",
    "db:reset": "bun run lib/db/scripts/db-reset.ts && bun run db:migrate && bun run db:seed",
    "email:dev": "email dev --dir emails --port 3001",
    "email:export": "email export --dir emails --out .react-email",
    "shadcn:update": "bun shadcn@canary add -a -y -o",
    "shadcn:check": "bun shadcn@canary diff",
    "knip": "knip"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^1.0.0",
    "@react-email/tailwind": "^2.0.0",
    "@sentry/nextjs": "^10.23.0",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-table": "^8.21.3",
    "@trpc/client": "^11.7.1",
    "@trpc/next": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@types/pg": "^8.15.6",
    "better-auth": "^1.3.34",
    "bullmq": "^5.63.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.24",
    "input-otp": "^1.4.2",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.554.0",
    "next": "^16.0.3",
    "next-themes": "^0.4.6",
    "pg": "^8.14.1",
    "postgres": "^3.4.5",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.2",
    "react-dom": "^19.2.0",
    "react-email": "^5.0.1",
    "react-hook-form": "^7.66.1",
    "react-resizable-panels": "^3.0.6",
    "recharts": "2.15.4",
    "resend": "^6.4.2",
    "sonner": "^2.0.7",
    "stripe": "^19.3.0",
    "superjson": "^2.2.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.9.0",
    "@react-email/preview-server": "5.0.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.3",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jsdom": "25.0.1",
    "knip": "^5.45.3",
    "lint-staged": "^15.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": "20.x"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings 0 --no-warn-ignored",
      "vitest related --run"
    ],
    "*.{yml,yaml,md,json}": [
      "prettier --write"
    ]
  }
}
